{
  "name": "freeter",
  "version": "2.7.1-beta",
  "main": "main.js",
  "homepage": "https://freeter.io/",
  "repository": "https://github.com/FreeterApp/Freeter.git",
  "author": "Alex Kaul",
  "license": "GPL-3.0-or-later",
  "description": "Organizer For Those Who Do",
  "scripts": {
    "dev": "cross-env REACT_DEVTOOLS=enabled yarn run dev:react-devtools",
    "dev:react-devtools": "concurrently \"react-devtools\" \"yarn run dev:no-react-devtools\"",
    "dev:no-react-devtools": "rimraf build && yarn run dev:assets && yarn run dev:preload && concurrently --names \"RENDERER,MAIN,RUN\" -c \"blue.bold,magenta.bold,green.bold\" \"yarn run dev:renderer\" \"yarn run dev:main\" \"yarn run dev:run\"",
    "dev:assets": "copyfiles --up 1 src/assets/**/* build/",
    "dev:preload": "cross-env NODE_ENV=development webpack --config webpack.preload.config.js",
    "dev:renderer": "cross-env NODE_ENV=development webpack-dev-server --config webpack.renderer.config.js",
    "dev:main": "cross-env NODE_ENV=development webpack --watch --config webpack.main.config.js",
    "dev:run": "cross-env NODE_ENV=development nodemon --on-change-only --watch build/main.js --exec \"sleep 3 && electron ./build/main.js --remote-debugging-port=9223 --inspect=5858\"",
    "prod": "rimraf build && yarn run prod:assets && concurrently --names \"RENDERER,MAIN,PRELOAD\" -c \"blue.bold,magenta.bold,green.bold\" \"yarn run prod:renderer\" \"yarn run prod:main\" \"yarn run prod:preload\"",
    "prod:assets": "copyfiles --up 1 src/assets/**/* build/",
    "prod:preload": "cross-env NODE_ENV=production webpack --config webpack.preload.config.js",
    "prod:renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.config.js",
    "prod:main": "cross-env NODE_ENV=production webpack --config webpack.main.config.js",
    "prod:run": "electron ./build/main.js",
    "package": "electron-builder --config electron-builder.config.js",
    "build:dev": "yarn run prod && electron-builder --config electron-builder-dev.config.js --mac dir",
    "install:dev": "yarn run build:dev && rm -rf /Applications/Freeter\\ Dev.app && cp -R dist/mac*/Freeter\\ Dev.app /Applications/",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --clearCache && cross-env JEST_HIDE_USAGE=1 jest --watch",
    "test:typecheck:common": "tsc --project ./tests/common/tsconfig.json --noEmit",
    "test:typecheck:main": "tsc --project ./tests/main/tsconfig.json --noEmit",
    "test:typecheck:renderer": "tsc --project ./tests/renderer/tsconfig.json --noEmit",
    "test:typecheck": "concurrently --names \"RENDERER,MAIN,COMMON\" -c \"blue.bold,magenta.bold,green.bold\" \"yarn run test:typecheck:renderer\" \"yarn run test:typecheck:main\" \"yarn run test:typecheck:common\"",
    "lint": "concurrently \"yarn run lint:common\" \"yarn run lint:main\" \"yarn run lint:renderer\" \"yarn run lint:tests:common\" \"yarn run lint:tests:main\" \"yarn run lint:tests:renderer\" \"yarn run lint:tests:utils\"",
    "lint:common": "cd src && cd common && eslint \"**/*.{ts,tsx}\"",
    "lint:common:fix": "cd src && cd common && eslint \"**/*.{ts,tsx}\" --fix",
    "lint:main": "cd src && cd main && eslint \"**/*.{ts,tsx}\"",
    "lint:main:fix": "cd src && cd main && eslint \"**/*.{ts,tsx}\" --fix",
    "lint:renderer": "cd src && cd renderer && eslint \"**/*.{ts,tsx}\"",
    "lint:renderer:fix": "cd src && cd renderer && eslint \"**/*.{ts,tsx}\" --fix",
    "lint:tests:common": "cd tests && cd common && eslint \"**/*.{ts,tsx}\"",
    "lint:tests:common:fix": "cd tests && cd common && eslint \"**/*.{ts,tsx}\" --fix",
    "lint:tests:main": "cd tests && cd main && eslint \"**/*.{ts,tsx}\"",
    "lint:tests:main:fix": "cd tests && cd main && eslint \"**/*.{ts,tsx}\" --fix",
    "lint:tests:renderer": "cd tests && cd renderer && eslint \"**/*.{ts,tsx}\"",
    "lint:tests:renderer:fix": "cd tests && cd renderer && eslint \"**/*.{ts,tsx}\" --fix",
    "lint:tests:utils": "cd tests && cd utils && eslint \"**/*.{ts,tsx}\"",
    "lint:tests:utils:fix": "cd tests && cd utils && eslint \"**/*.{ts,tsx}\" --fix",
    "cd:package-draft": "yarn run package",
    "draft-release": "yarn version && git push && git push --tags",
    "rebuild": "electron-rebuild -f -w node-pty"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@swc/core": "^1.4.0",
    "@swc/jest": "^0.2.36",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.56.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-loader": "^2.1.0",
    "nodemon": "^3.0.3",
    "react": "^18.2.0",
    "react-devtools": "^5.0.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "tiny-markdown-editor": "^0.1.31",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.8.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "node-pty": "https://github.com/microsoft/node-pty.git",
    "uuid": "^9.0.1",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "zustand": "^4.5.0"
  }
}
